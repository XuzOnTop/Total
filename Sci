local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Total Drama Island", "Ocean")

--Teleports
local Tab = Window:NewTab("Teleport")
local MainSection = Tab:NewSection("Teleports")

MainSection:NewButton("Lobby", "Teleports you to the lobby", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/2a3hN7BG"))()
end)

